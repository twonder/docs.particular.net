<!--startcode endpoint-mapping-appconfig-->
<configuration>
  <configSections>
    <section name="UnicastBusConfig"
             type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="assembly" 
           Endpoint="queue@machinename" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
</configuration>
